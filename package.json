{
  "name": "calendar-sync-os",
  "version": "1.0.0",
  "description": "CalendarSync OS - Multi-Google Calendar bidirectional sync system",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-session": "^1.18.2",
    "pg": "^8.11.3",
    "winston": "^3.19.0",
    "googleapis": "^129.0.0",
    "ioredis": "^5.3.2",
    "bullmq": "^5.3.0",
    "uuid": "^13.0.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@vercel/node": "^3.0.0"
  }
}
